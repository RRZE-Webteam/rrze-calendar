{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "rrze-calendar/events",
	"version": "1.2.0",
	"title": "Event List",
	"category": "rrze",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"featured": {
			"type": "boolean",
			"default": false
		},
		"display": {
			"type": "string",
			"default": "teaser"
		},
		"selectedCategories": {
			"type": "array",
			"default": {}
		},
		"selectedTags": {
			"type": "array",
			"default": {}
		},
		"numEvents": {
			"type": "integer",
			"default": 0
		},
		"pageLink": {
			"type": "string",
			"default": ""
		},
		"pageLinkLabel": {
			"type": "string",
			"default": "Show All Events"
		},
		"abonnementLink": {
			"type": "boolean",
			"default": false
		},
		"includeEvents": {
			"type": "string",
			"default": ""
		},
		"excludeEvents": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "rrze-calendar",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php"
}